@model IEnumerable<Invoice>
@{
    ViewData["Title"] = "Index";
}

<h1>Invoice List </h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Cliente</th>
            <th scope="col">Itebis</th>
            <th scope="col">Subtotal</th>
            <th scope="col">Total</th>
            <th scope="col">Editar</th>
            <th scope="col">Eliminar</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var items in Model)
        {
            <tr>
                <th scope="row">@items.Id</th>
                <td>@items.Customer</td>
                <td>@items.TotalItbis</td>
                <td>@items.SubTotal</td>
                <td>@items.Total</td>
                <td>@Html.ActionLink("Edita","Details","Invoice", new { @items.Id }) </td>
                <td>@Html.ActionLink("Eliminar","Delete","Invoice", new { @items.Id })</td>
            </tr>
        }

    </tbody>
</table>